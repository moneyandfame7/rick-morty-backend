{
  "name": "setup",
  "version": "1.0.0",
  "main": "dist/src/application/main.js",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "clearDist": "npx rimraf dist",
    "build": "npm run clearDist && npx tsc",
    "start": "node dist/src/application/main.js",
    "postinstall": "tsc",
    "dev": "nodemon src/application/main.ts.ts",
    "ts.check": "tsc --project tsconfig.json",
    "migrate-undo": "npx sequelize-cli db:migrate:undo:all",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate-status": "npx sequelize-cli db:migrate:status",
    "seed": "npx sequelize-cli db:seed:all",
    "add-build": "git add dist",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/express-fileupload": "^1.4.1",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.6",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@babel/register": "^7.18.9",
    "axios": "^1.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "env-var": "^7.3.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "node-fetch": "^3.3.0",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "process": "^0.11.10",
    "sequelize": "^6.28.0",
    "uuid": "^9.0.0"
  }
}
