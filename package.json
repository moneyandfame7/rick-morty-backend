  "scripts": {
    "clearDist": "npx rimraf dist",
    "build": "npm run clearDist && npx tsc",
    "start": "node build/src/application/main.js",
    "postinstall": "tsc",
    "dev": "nodemon src/application/main.ts",
    "ts.check": "tsc --project tsconfig.json",
    "migrate-undo": "npx sequelize-cli db:migrate:undo:all",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate-status": "npx sequelize-cli db:migrate:status",
    "seed": "npx sequelize-cli db:seed:all",
    "seed-debug": "npx sequelize-cli db:seed:all --debug",
    "seed-undo": "npx sequelize-cli db:seed:undo:all",
    "add-build": "git add dist",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "test": "echo \"Error: no test specified\" && exit 1"
